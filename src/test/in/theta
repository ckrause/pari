B=ellweierstrass(1/3+I,I/3+1);
for(i=1,8,print(B[i]))
/* Test some identities. Should all be close to 0 */
dtheta0(tau, d) = derivnum(z = 0, theta(z,tau,1), d);
exponent(B[4][1]*3 - dtheta0(I/3+1, 3)/dtheta0(I/3+1, 1))
T=thetanull(tau1,0);
[exponent(T[1]-eta((tau1+1)/2,1)^2/eta(tau1+1,1)),exponent(T[2]-eta(tau1/2,1)^2/eta(tau1,1)),exponent(T[3]-2*eta(2*tau1,1)^2/eta(tau1,1)),exponent(T[4]+2*Pi*eta(tau1,1)^3)]
[exponent(T[2]^4+T[3]^4-T[1]^4),exponent(T[1]*T[2]*T[3]-2*eta(tau1,1)^3)]
f(m)=T[2]^(4*m)+T[3]^(4*m)+(-1)^m*T[1]^(4*m);
[exponent(f(1)),exponent(f(2)-2*mfeval(mfinit([1,4]),mfEk(4),tau1)),exponent(f(3)+48*eta(tau1,1)^12),exponent(f(4)-f(2)^2/2),exponent(f(5)+80*eta(tau1,1)^12*mfeval(mfinit([1,4]),mfEk(4),tau1))]
