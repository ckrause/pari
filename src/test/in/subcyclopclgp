qp(m, p, flag = 0)=
{ my(d = quaddisc(m), f = lcm(d, p), H);
  H = vectorsmall(f, a, a % p == 1 && kronecker(d,a) > 0);
  znsubgroupgenerators(H,flag);
}
qp(36322, 5)
qp(36322, 5, 1)

