pr = idealadd(bnf,5,x-1);
g1 = gcharinit(bnf,idealpow(bnf,pr,2));
{c = gcharidentify(g1,[1,2],[
  [4,172.18925621407521183008840153751455639],
  [3,-172.18925621407521183008840153751455639]
])};
